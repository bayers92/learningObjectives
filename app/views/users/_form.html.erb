<%= simple_form_for(@user) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :blockyy1, inline_label: "Block 1" %>
    <%= f.input :blockyy2, inline_label: "Block 2" %>
    <%= f.input :blockyy3, inline_label: "Block 3" %>
    <%= f.input :blockyy4, inline_label: "Block 4" %>
    <%= f.input :blockyy5, inline_label: "Block 5" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
